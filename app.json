{
  "expo": {
    "name": "GatherGo",
    "slug": "gathergoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gglogo.png",
    "scheme": "gathergoapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./assets/images/gglogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#01082E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gathergo.dev",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/gglogo.png",
        "backgroundColor": "#01082E"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.gathergo.dev"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/gglogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/gglogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "946f8fac-e9ff-4515-8898-079f76ff558e"
      }
    }
  }
}
